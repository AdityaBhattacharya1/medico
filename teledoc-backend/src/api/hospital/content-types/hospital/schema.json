{
  "kind": "collectionType",
  "collectionName": "hospitals",
  "info": {
    "singularName": "hospital",
    "pluralName": "hospitals",
    "displayName": "Hospital",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Email": {
      "type": "email",
      "required": true
    },
    "Website": {
      "type": "string"
    },
    "Phone": {
      "type": "biginteger",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "hospital"
    },
    "Description": {
      "type": "richtext"
    },
    "Premium": {
      "type": "boolean",
      "required": true
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "hospitals"
    },
    "doctors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::doctor.doctor",
      "mappedBy": "hospital"
    }
  }
}
